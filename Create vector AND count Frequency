#include<bits/stdc++.h>
 using namespace std;
 int main()
{
   vector<int>kk;
   int n;
   cin>>n;
   
   for(int i=0;i<n;i++)
   {
       int x;
       cin>>x;
       kk.push_back(x);
   }

      
      map<int , int >m;
      
       
       
      for(long i=0;i<kk.size();i++)
        m[kk[i]]++;
        
       for(auto itr : m)
            cout<<itr.first<<" - "<<itr.second<<endl;
}
